{
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "build": "lerna run build --stream",
    "start": "lerna run start --stream",
    "check-types": "lerna run type-check --stream",
    "clean": "yarn workspaces foreach -A -R run clean || true && yarn clean:cache",
    "clean:cache": "yarn cache clean --all && rm -rf .yarn .pnp.cjs .pnp.loader.mjs node_modules yarn.lock",
    "lint": "lerna run lint --stream",
    "format": "lerna run format --stream"
  },
  "packageManager": "yarn@4.6.0",
  "engines": {
    "node": ">=20.18.2",
    "yarn": ">=4.6.0"
  },
  "devDependencies": {
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "~10.3.0",
    "lerna": "^8.1.9",
    "nx": "^20.4.2",
    "prettier": "3.6.2",
    "typescript": "^5.7.3",
    "vue": "^3.5.18",
    "vue-eslint-parser": "^9.4.3"
  }
}
